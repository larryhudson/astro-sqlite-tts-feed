
---
import Dump from "../../../components/Dump.astro";
import {getWikiPageSections} from "../../../utils/wikipedia";
const {pageId} = Astro.params;
const pageSections = await getWikiPageSections(pageId);
---

<h2>Sections</h2>
  {pageSections.map(section => (
  <details>
    <summary>Content (length: {section.textLength})</summary>
    <div set:html={section.content} />
  </details>
  <Dump data={section} />
  ))}
